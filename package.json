{
  "name": "npm-template-sync",
  "version": "0.0.0-semantic-release",
  "main": "dist/context.js",
  "module": "src/context.js",
  "description": "Keep npm package in sync with its template",
  "keywords": [
    "npm",
    "template"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "npm-template-sync": "bin/npm-template-sync"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "precover": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "docs": "documentation readme src/context.js --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c",
    "lint": "documentation lint src/context.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "aggregation-repository-provider": "^1.2.2",
    "babylon": "^6.18.0",
    "bitbucket-repository-provider": "^1.4.0",
    "caporal": "^0.10.0",
    "deep-extend": "^0.5.1",
    "expression-expander": "^6.1.0",
    "github-repository-provider": "^2.1.1",
    "js-yaml": "^3.11.0",
    "jsonpath": "^1.0.0",
    "local-repository-provider": "^1.2.14",
    "micromatch": "^3.1.10",
    "ora": "^2.0.0",
    "p-queue": "^2.4.1",
    "prompt": "^1.0.0",
    "recast": "^0.14.7",
    "set-getter": "^0.1.0",
    "simple-diff": "^1.6.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "documentation": "^6.3.2",
    "execa": "^0.10.0",
    "markdown-doctest": "^0.9.1",
    "mock-repository-provider": "^3.0.4",
    "nyc": "^11.7.1",
    "rollup": "^0.58.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-executable": "^1.2.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "semantic-release": "^15.2.0",
    "tempy": "^0.2.1",
    "travis-deploy-once": "^5.0.0"
  },
  "optionalDependencies": {
    "keytar": "^4.1.0"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync#readme",
  "release": {},
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
