{
  "name": "npm-template-sync",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/context.mjs",
  "module": "src/context.mjs",
  "description": "Keep npm package in sync with its template",
  "keywords": [
    "npm",
    "template",
    "scaffolding",
    "boilerplate",
    "git"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "npm-template-sync": "src/npm-template-sync-cli.mjs"
  },
  "scripts": {
    "test": "ava --node-arguments '--experimental-modules' --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --node-arguments '--experimental-modules' --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/context.mjs --section=API",
    "lint": "documentation lint src/context.mjs"
  },
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@iarna/toml": "^2.2.3",
    "aggregation-repository-provider": "^2.6.42",
    "babylon": "^6.18.0",
    "commander": "^4.1.1",
    "content-entry": "^2.3.1",
    "expression-expander": "^7.0.4",
    "github-repository-provider": "^7.6.7",
    "hinted-tree-merger": "^3.12.1",
    "js-yaml": "^3.13.1",
    "local-repository-provider": "^6.5.11",
    "loglevel-mixin": "^5.0.5",
    "markdown": "^0.5.0",
    "micromatch": "^4.0.2",
    "recast": "^0.18.7",
    "remark-parse": "^7.0.2",
    "remark-stringify": "^7.0.4",
    "remove-sensible-values": "^1.0.5",
    "unified": "^8.4.2"
  },
  "devDependencies": {
    "ava": "^3.5.0",
    "c8": "^7.1.0",
    "documentation": "^12.1.4",
    "execa": "^4.0.0",
    "mock-repository-provider": "^5.1.40",
    "semantic-release": "^17.0.4"
  },
  "engines": {
    "node": ">=13.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
