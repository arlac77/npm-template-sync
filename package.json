{
  "name": "npm-template-sync",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/context.js",
  "module": "src/context.js",
  "description": "Keep npm package in sync with its template",
  "keywords": [
    "npm",
    "template"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "npm-template-sync": "bin/npm-template-sync"
  },
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "pretest": "rollup -c",
    "posttest": "npm run prepare && markdown-doctest",
    "docs": "documentation readme src/context.js --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c",
    "lint": "documentation lint src/context.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "aggregation-repository-provider": "^2.0.35",
    "babylon": "^6.18.0",
    "bitbucket-repository-provider": "^2.1.5",
    "caporal": "^0.10.0",
    "deep-extend": "^0.6.0",
    "enquirer": "^2.0.7",
    "expression-expander": "^6.1.3",
    "github-repository-provider": "^4.0.35",
    "js-yaml": "^3.12.0",
    "jsonpath": "^1.0.0",
    "local-repository-provider": "^3.2.1",
    "micromatch": "^3.1.10",
    "recast": "^0.16.0",
    "repository-provider": "^7.0.0",
    "semver": "^5.6.0",
    "set-getter": "^0.1.0",
    "simple-diff": "^1.6.0"
  },
  "devDependencies": {
    "ava": "^1.0.0-rc.2",
    "documentation": "^8.1.2",
    "execa": "^1.0.0",
    "markdown-doctest": "^0.9.1",
    "mock-repository-provider": "^3.0.49",
    "rollup": "^0.67.1",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semantic-release": "^15.12.0",
    "travis-deploy-once": "^5.0.9",
    "c8": "^3.2.1",
    "esm": "^3.0.84"
  },
  "optionalDependencies": {
    "keytar": "^4.3.0"
  },
  "engines": {
    "node": ">=10.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync#readme",
  "release": {},
  "ava": {
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
