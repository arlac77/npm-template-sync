{
  "name": "npm-template-sync",
  "version": "0.0.0-semantic-release",
  "description": "Keep npm package in sync with its template",
  "main": "dist/npm-template-sync.js",
  "module": "src/npm-template-sync.js",
  "scripts": {
    "dev": "next",
    "start": "next start",
    "cover": "rollup -c tests/rollup.config.js && mocha --reporter tests/istanbul.reporter.js build/bundle.test.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md",
    "build": "next build && node_modules/rollup/bin/rollup --output=bin/npm-template-sync -c -- src/npm-template-sync.js && chmod +x bin/npm-template-sync",
    "test": "rollup -c tests/rollup.config.js && mocha --reporter tests/istanbul.reporter.js build/bundle.test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync.git"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "dependencies": {
    "deep-extend": "^0.4.1",
    "expression-expander": "^5.2.9",
    "js-yaml": "^3.7.0",
    "keychain": "^1.0.0",
    "next": "^1.2.3",
    "octonode": "^0.7.7",
    "prompt": "^1.0.0",
    "pull-request": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cracks": "3.1.2",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.2.0",
    "rollup": "^0.37.2",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "semantic-release": "^6.3.5"
  },
  "release": {
    "verifyRelease": "cracks"
  },
  "keywords": [
    "npm",
    "template"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "contributors": [{
    "name": "Markus Felten",
    "email": "markus.felten@gmx.de"
  }]
}
