{
  "name": "npm-template-sync",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/context.mjs",
  "module": "src/context.mjs",
  "description": "Keep npm package in sync with its template",
  "keywords": [
    "npm",
    "template",
    "scaffolding"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "npm-template-sync": "bin/npm-template-sync"
  },
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/context.mjs --section=API",
    "lint": "documentation lint src/context.mjs",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "pretest": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "aggregation-repository-provider": "^2.6.32",
    "babylon": "^6.18.0",
    "commander": "^4.0.1",
    "content-entry": "^2.2.2",
    "expression-expander": "^7.0.3",
    "github-repository-provider": "^7.4.30",
    "hinted-tree-merger": "^3.1.0",
    "js-yaml": "^3.13.1",
    "local-repository-provider": "^6.4.51",
    "loglevel-mixin": "^5.0.0",
    "micromatch": "^4.0.2",
    "recast": "^0.18.5",
    "remove-sensible-values": "1.0.3",
    "semver": "^6.3.0",
    "simple-diff": "^1.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "ava": "^2.4.0",
    "builtin-modules": "^3.1.0",
    "c8": "^6.0.1",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "execa": "^3.4.0",
    "markdown-doctest": "^1.0.0",
    "mock-repository-provider": "^5.1.31",
    "rollup": "^1.27.8",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-executable": "^1.5.1",
    "semantic-release": "^15.13.31"
  },
  "engines": {
    "node": ">=12.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.mjs"
    ],
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
