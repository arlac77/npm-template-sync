{
  "name": "npm-template-sync",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/context.js",
  "module": "src/context.js",
  "description": "Keep npm package in sync with its template",
  "keywords": [
    "npm",
    "template"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "npm-template-sync": "bin/npm-template-sync"
  },
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/context.js --section=API",
    "lint": "documentation lint src/context.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "pretest": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "aggregation-repository-provider": "^2.1.8",
    "babylon": "^6.18.0",
    "caporal": "^1.1.0",
    "content-entry": "^2.0.2",
    "deep-extend": "^0.6.0",
    "enquirer": "^2.2.0",
    "expression-expander": "^6.1.3",
    "github-repository-provider": "^5.1.18",
    "js-yaml": "^3.12.0",
    "jsonpath": "^1.0.0",
    "local-repository-provider": "^4.0.17",
    "loglevel-mixin": "^3.0.0",
    "micromatch": "^3.1.10",
    "recast": "^0.16.1",
    "semver": "^5.6.0",
    "set-getter": "^0.1.0",
    "simple-diff": "^1.6.0"
  },
  "devDependencies": {
    "ava": "^1.0.1",
    "c8": "^3.2.1",
    "documentation": "^9.1.1",
    "esm": "^3.0.84",
    "execa": "^1.0.0",
    "markdown-doctest": "^0.9.1",
    "mock-repository-provider": "^4.0.17",
    "rollup": "^1.0.0",
    "rollup-plugin-cleanup": "^3.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "semantic-release": "^15.13.2"
  },
  "optionalDependencies": {
    "keytar": "^4.3.0"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync#readme",
  "release": {},
  "ava": {
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
