{
  "name": "npm-template-sync",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/context.mjs",
  "exports": {
    ".": "./src/context.mjs"
  },
  "description": "Keep npm package in sync with its template",
  "keywords": [
    "npm",
    "template",
    "scaffolding",
    "boilerplate",
    "git"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "npm-template-sync": "src/npm-template-sync-cli.mjs"
  },
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/context.mjs --section=API",
    "lint": "documentation lint src/context.mjs"
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@iarna/toml": "3.0.0",
    "aggregation-repository-provider": "^2.6.57",
    "babylon": "^6.18.0",
    "commander": "^5.1.0",
    "content-entry": "^2.3.4",
    "expression-expander": "^7.0.5",
    "github-repository-provider": "^7.9.7",
    "hinted-tree-merger": "^3.12.7",
    "js-yaml": "^3.13.1",
    "local-repository-provider": "^6.5.27",
    "loglevel-mixin": "^5.0.6",
    "markdown": "^0.5.0",
    "micromatch": "^4.0.2",
    "one-time-execution-method": "^2.0.1",
    "recast": "^0.19.0",
    "remark-parse": "^8.0.2",
    "remark-stringify": "^8.0.0",
    "remove-sensible-values": "^1.0.6",
    "unified": "^9.0.0"
  },
  "devDependencies": {
    "ava": "^3.8.1",
    "c8": "^7.1.1",
    "documentation": "^12.3.0",
    "execa": "^4.0.0",
    "mock-repository-provider": "^5.1.56",
    "semantic-release": "^17.0.7"
  },
  "engines": {
    "node": ">=14.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
